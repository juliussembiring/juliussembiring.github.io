        "# b = daging sapi\n",
        "# c = sayur bayam\n",
        "# d = susu\n",
        "\n",
        "a = int(2*20000)\n",
        "b = int(3*100000)\n",
        "c = int(2*10000)\n",
        "d = int(1*15000)\n",
        "\n",
        "result = a+b+c+d\n",
        "print(result)"
      ],
